<div class="panel panel-primary">
	<div class="panel-heading">Ajudas técnicas</div>
	<ul class="list-group">
		<li class="list-group-item">
			<span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span> 
			<a href="<%= ajudas_tecnicas_patients_url() %>">Utentes</a>
			<br />

			<span class="glyphicon glyphicon-plus" aria-hidden="true"></span> 
			<a href="<%= new_ajudas_tecnicas_patient_url() %>">Novo utente</a>
		</li>
		<% if current_user.administrator || current_user.ajudas_tecnicas_user.nivel_acesso == 2 %>
