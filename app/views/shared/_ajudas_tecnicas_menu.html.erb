			<li class="list-group-item">
				<span class="glyphicon glyphicon-envelope" aria-hidden="true"></span> 
				<a href="<%= ajudas_tecnicas_contact_types_path() %>">Tipos de contacto</a>
			</li>

			<li class="list-group-item">
