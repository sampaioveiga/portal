<%= form_for(@equipment) do |f| %>

	<%= f.label :equipamento %>
	<%= f.text_field :equipamento, class: 'form-control' %>

	<%= f.label :inventario %>
	<%= f.text_field :inventario, class: 'form-control' %>

	<%= f.label :descricao %>
	<%= f.text_field :descricao, class: 'form-control' %>

	<%= f.submit 'Submeter', class: 'btn btn-success' %>

<% end %>