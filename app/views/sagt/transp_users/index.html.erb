<aside class="col-md-3">
	<%= render 'shared/menu_transp' %>
</aside>
<div class="col-md-9">
	<h1>Utilizadores autorizados</h1>
	<table class="table">
		<thead>
			<tr>
				<th>Utilizador</th>
				<th>Acesso</th>
			</tr>
		</thead>
		<tbody>
			<% @users.each do |user| %>
				<tr>
					<td>
						<%= link_to user.nome_utilizador, edit_sagt_transp_user_path(user.transp_user.id) %>
					</td>
					<td>
						<%= render 'shared/nivel_acesso', user: user.transp_user %>
					</td>
				</tr>
			<% end %>
		</tbody>
	</table>
	<%= render 'new' %>
</div>