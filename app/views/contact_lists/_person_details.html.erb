<address>
	<strong><%= @person.titulo %> <%= @person.nome_utilizador %></strong><br>
	<% @person.ulsne_departments.each do |ulsne_department| %>
		<%= ulsne_department.nome_servico %><br />
	<% end %>
	<%= @person.ulsne_site.nome_unidade %><br />
	<abbr title="Email">Email:</abbr> <%= @person.email %><br />
	<% @person.user_phone_numbers.each do |phone_number| %>
		<abbr title="Telefone">T:</abbr> <%= phone_number.numero_contacto %><br />
	<% end %>
</address>