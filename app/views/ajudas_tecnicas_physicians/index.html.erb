<aside class="col-md-2">
	<%= render 'shared/ajudas_tecnicas_menu' %>
</aside>
<div class="col-md-10">
	<h1>Médicos de família</h1>
	<div class="col-md-8">
		<table class="table">
			<thead>
				<tr>
					<th>Nome</th>
					<th></th>
				</tr>
			</thead>
			<tbody>
				<% @physicians.each do |p| %>
					<tr>
						<td>Dr. <%= p.nome_medico %></td>
						<td><a href="<%= edit_ajudas_tecnicas_physician_path(p) %>"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span></a></td>
					</tr>
				<% end %>
			</tbody>
		</table>
	</div>
	<div class="col-md-4 well">
		<h3>Novo médico</h3>
		<%= render 'form' %>
	</div>
</div>