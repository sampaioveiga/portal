<div role="tabpanel" class="tab-pane fade" id="pneumo">
	<table class="table">
		<thead>
			<tr>
				<th>Data</th>
				<th>Utilizador</th>
			</tr>
		</thead>
		<tbody>
			<% @patient.pneumology_forms.reorder("data DESC").each do |pf| %>
				<tr>
					<td><%= link_to pf.data, patient_pneumology_form_path(@patient, pf), remote: true %></td>
					<td><%= pf.user.nome_utilizador %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>