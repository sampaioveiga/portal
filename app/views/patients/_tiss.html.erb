<div role="tabpanel" class="tab-pane fade" id="tiss">
	<div class="row">
		<div class="col-md-6">
			<table class="table">
				<thead>
					<tr>
						<!--<th>User</th>-->
						<th>Data</th>
						<th>Valor</th>
					</tr>
				</thead>
				<tbody>
					<%= render @patient.uci_tiss_values.order(data: :desc) %>
				</tbody>
			</table>
		</div>
		<div class="col-md-6">
			<%= line_chart @patient.uci_tiss_values.group_by_day(:data, last: 30).sum(:valor), width: "100%" unless @patient.uci_tiss_values.count == 0 %>
		</div>
	</div>
</div>