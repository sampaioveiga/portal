<aside class="col-md-2">
	<%= render 'shared/menu_uci_patients' %>
</aside>
<div class="col-md-10">
	<h1>Editar <%= @user.user.nome_utilizador %></h1>
	<%= render 'form' %>
	<%= link_to "Eliminar", uci_user_path(@user),
				method: :delete,
				data: { confirm: 'Tem a certeza que deseja eliminar o utilizador?' },
				class: 'btn btn-danger' %>
</div>