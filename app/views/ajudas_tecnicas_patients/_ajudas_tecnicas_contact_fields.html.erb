<div class="nested-fields">
	<div class="form-group">
		<%= f.label :tipo_contacto %>
		<%= f.collection_select(:ajudas_tecnicas_contact_type_id, AjudasTecnicasContactType.order(:tipo_contacto), :id, :tipo_contacto, {}, { class: 'form-control'}) %>
	</div>
	<div class="form-group">
		<%= f.label :contacto %>
		<%= f.text_field :contacto, class: 'form-control' %>
	</div>
	<%= link_to_remove_association "Remover contacto", f %>
</div>