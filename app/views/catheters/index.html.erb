<aside class="col-md-2">
	<%= render 'shared/menu_patients' %>
</aside>
<div class="col-md-10">
	<h1>Tipos de cateter</h1>
	<table class="table">
		<thead>
			<tr>
				<th></th>
			</tr>
		</thead>
		<tbody>
			<% @catheters.each do |c| %>
				<tr>
					<td><%= link_to c.nome_cateter, edit_catheter_path(c) %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
	<div class="panel panel-default">
		<div class="panel-heading">Adicionar tipo de cateter</div>
		<div class="panel-body">
			<%= render 'form' %>
		</div>
	</div>
</div>